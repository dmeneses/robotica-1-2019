
public class Morse {
	public static String convertirAMorse(String frase){
	String letras = "abcdefghijklmnopqrstuvwxyz._";
	frase = frase.toLowerCase();
	
	String simboloLetra[] = new String[27];
	
	simboloLetra[0] = ".-"; 		//a
	simboloLetra[1] = "-...";		//b
	simboloLetra[2] = "-.-.";		//c
	simboloLetra[3] = "-..";		//d
	simboloLetra[4] = ".";		    //e
	simboloLetra[5] = "..-.";		//f
	simboloLetra[6] = "--.";		//g
	simboloLetra[7] = "....";		//h
	simboloLetra[8] = "..";		    //i
	simboloLetra[9] = ".---";		//j
	simboloLetra[10] = "-.-";		//k
	simboloLetra[11] = ".-..";	    //l
	simboloLetra[12] = "--";		//m
	simboloLetra[13] = "-.";		//n
	simboloLetra[14] = "---";		//o
	simboloLetra[15] = ".--.";	    //p
	simboloLetra[16] = "--.-";	    //q
	simboloLetra[17] = ".-.";		//r
	simboloLetra[18] = "...";		//s
	simboloLetra[19] = "-";		    //t
	simboloLetra[20] = "..-";		//u
	simboloLetra[21] = "...-";	    //v
	simboloLetra[22] = ".--";		//w
	simboloLetra[23] = "-..-";	    //x
	simboloLetra[24] = "-.--";	    //y
	simboloLetra[25] = "--..";	    //z
	simboloLetra[26] = ".-.-.-";	//.
	simboloLetra[27] = "..--.-";	//_
	
	String morse = " ";
	
	
	
	}

}
