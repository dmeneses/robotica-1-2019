package clases;

public class Auto {
	
	String color;
	String marca;
	int velocidad;
	
	public Auto( String uncolor,String unamarca,int unavelocidad ){
		color = uncolor;
		marca = unamarca;
		velocidad = unavelocidad;
   
		
	
		
		
	}

	public void arrancar(){
		
		System.out.println(marca);
		
	}
	
	public String color(){
		
		System.out.println(color);
		return color;
		
	}
	
    public int velocidad(){
		
		System.out.println(velocidad);
		return velocidad;
	
		
    }
		
	

}
