import java.awt.Color;


public class sensor {
		public static String colorName(int color)
		{
			switch (color)
			{
				case Color.BITMASK:
					return "None";
					
				case Color.BLACK:
					return "Black";
					
				case Color.BLUE:
					return "Blue";
					
				case Color.BROWN:
					return "Brown";
					
				case Color.CYAN:
					return "Cyan";
					
				case Color.DARK_GRAY:
					return "Dark Gray";
					
				case Color.GRAY:
					return "Gray";
					
				case Color.GREEN:
					return "Green";
					
				case Color.LIGHT_GRAY:
					return "Light Gray";
					
				case Color.MAGENTA:
					return "Magenta";
					
				case Color.ORANGE:
					return "Orange";
					
				case Color.PINK:
					return "Pink";
					
				case Color.RED:
					return "Red";
					
				case Color.WHITE:
					return "White";
					
				case Color.YELLOW:
					return "Yellow";
			}
			
			return "";
		}
	

	}

