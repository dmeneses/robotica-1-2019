
public class jarvis
{

	public static void main(String[] args)
	
	{
		//viernes we = new viernes();
		traductor tr = new traductor();
		
       
		while(true)
        {
		    double yo = tr.medir();
       
           if(yo > 0)
           {
    	      tr.traducir();
           }		
        
        } 
		    
     }
}
