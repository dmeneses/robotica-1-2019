import lejos.hardware.Button;


public class MorseSkynet {

	public static void main(String[] args) {
		
		
		
		
		
		
		
		TraductorString convertidor = new TraductorString ();
		convertidor.declarar("A",".-");
		convertidor.declarar("B","-..");
		convertidor.declarar("C","-.-.");
		convertidor.declarar("D","-..");
		convertidor.declarar("E",".");
		convertidor.declarar("F","..-.");
		convertidor.declarar("G","--.");
		convertidor.declarar("H","....");
		convertidor.declarar("I","..");
		convertidor.declarar("J",".---");
		convertidor.declarar("K","-.-");
		convertidor.declarar("L",".-..");
		convertidor.declarar("M","--");
		convertidor.declarar("N","-.");
		convertidor.declarar("O","---");
		convertidor.declarar("P",".--.");
		convertidor.declarar("Q","--.-");
		convertidor.declarar("R",".-.");
		convertidor.declarar("S","...");
		convertidor.declarar("T","-");
		convertidor.declarar("U","..-");
		convertidor.declarar("V","...-");
		convertidor.declarar("W",".--");
		convertidor.declarar("X","-..-");
		convertidor.declarar("Y","-.--");
		convertidor.declarar("Z","--..");
		 new MorseSkynet().Inicio(convertidor);
	}

	private void Inicio(TraductorString convertidor) {
		System.out.println("Presione cualquier boton para iniciar");
		Button.waitForAnyPress();
		//int PresioneSensor=
		 
	}

}
